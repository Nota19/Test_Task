<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="ru.hmel.model.ElectroEmployee" />
	<import class="ru.hmel.model.Electronics" />
	<import class="ru.hmel.model.ElectroType" />
	<import class="ru.hmel.model.Employee" />
	<import class="ru.hmel.model.PositionType" />
	<import class="ru.hmel.model.Purchase" />
	<import class="ru.hmel.model.PurchaseType" />
	<class name="ru.hmel.model.impl.ElectroEmployeeImpl" table="store_ElectroEmployee">
		<cache usage="read-write" />
		<composite-id name="primaryKey" class="ru.hmel.service.persistence.ElectroEmployeePK">
			<key-property name="electro_employee_id" type="com.liferay.portal.dao.orm.hibernate.LongType" />
			<key-property name="etype" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		</composite-id>
	</class>
	<class name="ru.hmel.model.impl.ElectronicsImpl" table="store_Electronics">
		<cache usage="read-write" />
		<id name="electronics_id" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="etype" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="price" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="count" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="inStock" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="archive" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="ru.hmel.model.impl.ElectroTypeImpl" table="store_ElectroType">
		<cache usage="read-write" />
		<id name="electro_type_id" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="ru.hmel.model.impl.EmployeeImpl" table="store_Employee">
		<cache usage="read-write" />
		<id name="employee_id" type="long">
			<generator class="assigned" />
		</id>
		<property name="lastname" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="firstname" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="patronymic" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="birthdate" type="org.hibernate.type.TimestampType" />
		<property name="position" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="gender" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
	</class>
	<class name="ru.hmel.model.impl.PositionTypeImpl" table="store_PositionType">
		<cache usage="read-write" />
		<id name="position_id" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="ru.hmel.model.impl.PurchaseImpl" table="store_Purchase">
		<cache usage="read-write" />
		<id name="purchase_id" type="long">
			<generator class="assigned" />
		</id>
		<property name="electro_id" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="employee_id" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="purchaseDate" type="org.hibernate.type.TimestampType" />
		<property name="type" type="com.liferay.portal.dao.orm.hibernate.LongType" column="type_" />
	</class>
	<class name="ru.hmel.model.impl.PurchaseTypeImpl" table="store_PurchaseType">
		<cache usage="read-write" />
		<id name="purchase_type_id" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>